{
  "name": "coc-calc",
  "version": "1.6.0",
  "description": "calculate extension for coc.nvim",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim",
    "calculator",
    "calc"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "prepare": "yarn run clean && yarn run build",
    "test": "tslint -p ."
  },
  "files": [
    "lib"
  ],
  "engines": {
    "coc": "^0.0.71"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Calc",
      "properties": {
        "calc.debug": {
          "type": "boolean",
          "default": false
        },
        "calc.replaceOriginalExpression": {
          "type": "boolean",
          "default": true,
          "description": "Enable relace original expression"
        },
        "calc.priority": {
          "type": "integer",
          "default": 1000,
          "description": "Calc priority"
        },
        "calc.highlight": {
          "type": "boolean",
          "description": "Enable calc highlight",
          "default": true
        }
      }
    }
  },
  "license": "MIT",
  "author": "weirongxu.raidou@gmail.com",
  "publisher": "weirongxu",
  "homepage": "https://github.com/weirongxu/coc-calc",
  "repository": "https://github.com/weirongxu/coc-calc.git",
  "devDependencies": {
    "@types/node": "^12.7.11",
    "coc.nvim": "^0.0.73",
    "editor-calc": "^1.1.0",
    "ts-loader": "^6.2.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "vscode-languageserver-protocol": "^3.14.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {}
}
